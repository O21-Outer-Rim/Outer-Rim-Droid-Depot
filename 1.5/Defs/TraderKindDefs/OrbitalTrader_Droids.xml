					<li>OuterRim_KXSecurityDroid</li>
					<li>OuterRim_ProtocolDroid</li>
					<li>OuterRim_TacticalDroid</li>
				</pawnKinds>
				<hediffs>
					<li>OuterRim_RestraintBolt</li>
				</hediffs>
			</li>
			<!-- Exotic (Combat/Other) -->
			<li Class="Asimov.StockGenerator_Automatons">
				<countRange>-3~1</countRange>
